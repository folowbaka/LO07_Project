{% extends "UTTEtuBundle::layout.html.twig" %}


{% block etu_body %}

    <h2>Ajouter Etudiant</h2>

    {{ form_start(form) }}

    <div class="form-group">{{ form_row(form.idEtudiant, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.nom, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.prenom, {'attr':{'class':'form-control'}}) }}</div>

    <button type="submit">Valider</button>
    <button type="reset">Reset</button>

    {{ form_end(form) }}

{% endblock %}