{% extends "UTTEtuBundle::layout.html.twig" %}


{% block etu_body %}
<div class="row">
    <h2>Fiche Etudiant</h2>
</div>
    {{ form_start(form) }}
    <div class="form-group">{{ form_row(form.nom, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.prenom, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.admission, {'attr':{'class':'form-control'}}) }}</div>
    <div class="form-group">{{ form_row(form.filliere, {'attr':{'class':'form-control'}}) }}</div>

    {{ form_end(form) }}

{% endblock %}